    "watch": "mocha --watch --compilers js:babel/register 'src/**/_spec/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cullophid/date-fp"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-loader": "^5.3.2",
    "eslint": "^1.7.1",
    "eslint-plugin-react": "^3.6.2",
    "mocha": "^2.3.3",
    "uglifyjs": "^2.4.10",
    "webpack": "^1.12.2"
  },
  "files": [
    "README.md",
    "build",
    "dist"
  ],
