{
  "name": "date-fp",
  "version": "1.1.5",
  "description": "Functional programming date management.",
  "main": "dist/index.js",
  "dependencies": {
    "lodash.curry": "^3.0.2"
  },
  "scripts": {
    "buildBrowser": "browserify browser.js -t babelify --outfile build/date-fp.js",
    "transpile": "babel src -d dist",
    "uglify": "uglify -s build/date-fp.js -o build/date-fp.min.js",
    "build": "npm run transpile && npm run buildBrowser && npm run uglify",
    "test": "mocha --compilers js:babel/register 'src/**/_spec/*.js'",
    "testBuild": "mocha buildTest.js",
    "watch": "mocha --watch --compilers js:babel/register 'src/**/_spec/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cullophid/date-fp"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "mocha": "^2.3.3",
    "uglify": "^0.1.5"
  },
  "author": "",
  "license": "ISC"
}
